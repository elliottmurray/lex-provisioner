{

    "version": "0.2.0",
    "configurations": [
      {
          "name": "Python: SAM",
          "type": "python",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "port": 5890,
          "host": "localhost",
          "pathMappings": [
              {
                  "localRoot": "${workspaceFolder}/src",
                  "remoteRoot": "/var/task"
              }
          ]
      }
    ]
}
